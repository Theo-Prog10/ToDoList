<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>ToDoList</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.8"/>
        <PackageReference Include="Microsoft.EntityFrameworkCore.InMemory" Version="9.0.0-rc.2.24474.1" />
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0"/>
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Model\requests\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Model\requests\**" />
    </ItemGroup>

    <ItemGroup>
      <Content Remove="Model\requests\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Model\requests\**" />
    </ItemGroup>

</Project>
